[Icon Theme]
Name=SE98 KDE
Comment=Addon for Win98SE, follows colorscheme.

Example=user-desktop
Inherits=SE98,Breeze,hicolor

FollowsColorScheme=true

# KDE Specific Stuff
DesktopDefault=48
DesktopSizes=16,22,24,32,48,64
ToolbarDefault=22
