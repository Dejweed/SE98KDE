SmallSizes=16,22,24,32,48,64
PanelDefault=64
PanelSizes=16,22,24,32,48,64
DialogDefault=32
DialogSizes=16,22,24,32,48,64
LinkOverlay=link_overlay
LockOverlay=lock_overlay
